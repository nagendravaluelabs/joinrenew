{{#ember-chosen name=(concat namePrefix '_country') id=(concat idPrefix '_country') class=(concat class ' form-select select-chosen') selectionDidChange=actionHooks value=mutable chosenValueChanged=chosenValueChanged}}
  <option value="">Select</option>
  {{#each genericData.generic.country as |country|}}
    {{#if (isEqualTo (jsStrings country.countrykey 'upper') (jsStrings selected 'upper'))}}
      <option value="{{country.countrykey}}" selected>{{country.countrycode}}</option>
    {{else}}
      <option value="{{country.countrykey}}">{{country.countrycode}}</option>
    {{/if}}
  {{/each}}
{{/ember-chosen}}